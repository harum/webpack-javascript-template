{
  "name": "simple-serverside",
  "version": "1.0.0",
  "description": "Simple Serverside Javasccript Template",
  "private": true,
  "scripts": {
    "test": "jest",
    "watch": "webpack --watch --config config/webpack.config.development.js",
    "dev": "webpack-dev-server --open --config config/webpack.config.development.js",
    "build": "webpack --config config/webpack.config.production.js",
    "start": "http-server dist -p 3030",
    "serve": "yarn build && yarn start"
  },
  "author": "Putu Harum Bawa",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.8.3",
    "@babel/core": "^7.8.3",
    "@babel/node": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "babel-jest": "^25.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.4.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-import": "^2.20.0",
    "favicons-webpack-plugin": "^2.1.0",
    "file-loader": "^5.1.0",
    "html-webpack-plugin": "^3.2.0",
    "http-server": "^0.12.1",
    "husky": "^4.2.1",
    "jest": "^25.1.0",
    "lint-staged": "^10.0.2",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^7.0.0",
    "nodemon": "^2.0.2",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "1.19.1",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.3",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-merge": "^4.2.2",
    "workbox-webpack-plugin": "^5.0.0"
  },
  "dependencies": {
    "cssnano": "^4.1.10"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "*.{js,css,md}": "prettier --write"
  }
}
